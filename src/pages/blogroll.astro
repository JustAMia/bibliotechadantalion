---
import BaseLayout from "../layouts/baselayout.astro";
import BlogrollFeed from "src/components//blogroll/blogroll-feed.astro";

import { frontmatter } from "../data/blogroll.mdx";
---

<BaseLayout>
  <h1>Blogroll</h1>
  <p>The BlogrollFeed component takes as parameter an array of RSS urls</p>
  {frontmatter.urls && <BlogrollFeed urls={frontmatter.urls} />}
</BaseLayout>